<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.entando.entando7.plugins</groupId>
        <artifactId>entando7-plugins-engine-parent</artifactId>
        <version>7.2.2.0-rc1</version>
    </parent>
    <groupId>org.entando.entando7.plugins</groupId>
    <artifactId>entando7-plugins-cds-engine</artifactId>
    <version>7.2.2.0-rc1</version>
    <packaging>jar</packaging>
    <name>eN7 Plugin ENGINE - Content Delivery Server Connector</name>
    <description>Enables the use of Content Delivery Server</description>
    <url>http://www.entando.com/</url>
    <build>
        <resources>
            <resource>
                <directory>src/main/resources</directory>
                <excludes>
                    <exclude>src/main/resources/component</exclude>
                </excludes>
            </resource>
            <resource>
                <directory>src/main/resources</directory>
                <includes>
                    <include>**/*component.xml</include>
                </includes>
                <filtering>true</filtering>
            </resource>
            <resource>
                <directory>src/main/java</directory>
                <includes>
                    <include>**/*.properties</include>
                    <include>**/*.xml</include>
                    <include>**/*.xsd</include>
                    <include>**/*.txt</include>
                </includes>
            </resource>
            <resource>
                <directory>src/test/java</directory>
                <includes>
                    <include>**/*.sql</include>
                    <include>**/*.json</include>
                </includes>
            </resource>
            <resource>
                <directory>src/main/tld</directory>
                <targetPath>META-INF</targetPath>
                <includes>
                    <include>**/*.tld</include>
                </includes>
            </resource>
        </resources>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-deploy-plugin</artifactId>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <configuration>
                    <environmentVariables>
                        <ENTANDO_TENANTS>
<![CDATA[
[{
    "tenantCode": "tenant",
    "kcEnabled": true,
    "kcAuthUrl": "http://10-219-168-112.nip.io/auth",
    "kcRealm": "entando",
    "kcClientId": "quickstart",
    "kcClientSecret": "2837772a-f583-4dd5-8d89-9147a89dc60e",
    "kcPublicClientId": "entando-web",
    "kcSecureUris": "",
    "kcDefaultAuthorizations": "",
    "dbDriverClassName": "org.postgresql.Driver",
    "dbUrl": "jdbc:postgresql://testDbServer:5432/tenantDb2",
    "dbUsername": "db_user_1",
    "dbPassword": "db_password_1",
    "cdsPublicUrl": "http://cds.10-219-168-112.nip.io/tenant1",
    "cdsPrivateUrl": "http://tenant1-cds:8080",
    "cdsPath": "api/v1"
}]
]]>
                        </ENTANDO_TENANTS>
                        <CDS_ENABLED>true</CDS_ENABLED>
                        <CDS_PATH>/api/v1</CDS_PATH>
                        <KEYCLOAK_REALM>entando</KEYCLOAK_REALM>
                        <KEYCLOAK_CLIENT_ID>quickstart</KEYCLOAK_CLIENT_ID>
                    </environmentVariables>
                    <trimStackTrace>false</trimStackTrace>
                </configuration>
            </plugin>
        </plugins>
    </build>
